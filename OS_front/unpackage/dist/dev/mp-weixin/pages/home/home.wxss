.swiper {
  width: 100%;
  height: 192rpx;
}
.swiper image {
  width: 100%;
  height: 100%;
}
.nav {
  width: 100%;
  height: 500rpx;
  display: flex;
  flex-wrap: wrap;
}
.nav .nav-item {
  width: 50%;
  height: 50%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  border: 1px solid #ebedf0;
}
.nav .nav-item .nav-item-text {
  font-size: 32rpx;
  color: #25abbe;
  padding-top: 10rpx;
}

