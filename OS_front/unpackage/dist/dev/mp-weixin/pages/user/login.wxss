/* 水平间距 */
.login-box {
  width: 100%;
  height: 100%;
  position: relative;
  background: linear-gradient(30deg, #52bbc8 0%, #25ABBE 100%);
}
.login-box .login, .login-box .register {
  position: absolute;
}
.login-box .login .box, .login-box .register .box {
  box-shadow: 0px 0px 60rpx 0rpx rgba(100, 100, 100, 0.4);
}

