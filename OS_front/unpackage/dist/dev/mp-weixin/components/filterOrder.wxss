/* 水平间距 */
.filterOrder {
  z-index: 1;
  position: fixed;
  width: 100%;
  box-shadow: 0 6rpx 8rpx #dbdbdb;
}
.filterOrder .filterOrder-box {
  display: flex;
  background-color: #fff;
  height: 70rpx;
  align-items: center;
  justify-content: space-around;
}
.filterOrder .filterOrder-box view {
  width: 20%;
  text-align: center;
}
.filterOrder .filterOrder-box view.active {
  color: #25abbe;
  font-weight: 700;
}

