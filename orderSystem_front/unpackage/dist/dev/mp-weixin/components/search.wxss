/* 水平间距 */
.search {
  width: 100%;
  height: 70rpx;
  display: flex;
  position: fixed;
  top: 0;
  justify-content: center;
  align-items: center;
  background-color: #fff;
  z-index: 10;
}
.search .search-inner {
  display: flex;
  align-items: center;
  width: 90%;
  height: 80%;
  padding: 0 3%;
  box-sizing: border-box;
  border-radius: 50rpx;
  background-color: #F4F5F0;
}
.search .search-inner .search-text {
  width: 100%;
  font-size: 30rpx;
  color: #ACADA9;
}

